{
  "name": "kirby-content-translator",
  "type": "module",
  "version": "2.1.2",
  "private": true,
  "packageManager": "pnpm@9.2.0",
  "scripts": {
    "dev": "PLAYGROUND=true kirbyup serve src/panel/index.js",
    "build": "kirbyup src/panel/index.js",
    "build:playground": "PLAYGROUND=true pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier \"src/panel/**\" --write",
    "release": "pnpm run build && bumpp --all --execute \"npx zx scripts/pre-release.mjs\""
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@kirby-tools/licensing": "^0.1.2",
    "@sindresorhus/slugify": "^2.2.1",
    "autoprefixer": "^10.4.19",
    "bumpp": "^9.4.1",
    "destr": "^2.0.3",
    "eslint": "^9.4.0",
    "kirbyup": "^3.1.6",
    "kirbyuse": "^0.4.1",
    "ofetch": "^1.3.4",
    "p-all": "^5.0.0",
    "prettier": "^3.3.1",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "tailwindcss": "^3.4.3",
    "ufo": "^1.5.3",
    "zx": "^8.1.2"
  }
}
